{
  "name": "cx-redux-utils",
  "author": "Kyle Chamberlain",
  "version": "1.0.0",
  "license": "ISC",
  "description": "Tools for accommodating a specific redux pattern",
  "main": "lib/index.js",
  "repository": {
    "type": "git"
  },
  "scripts": {
    "build-docs": "./node_modules/.bin/jsdoc --configure .jsdoc.json --verbose",
    "clean-docs": "rm -rf ./docs",
    "compile": "./node_modules/.bin/babel -d lib/ src/",
    "docs": "npm run clean-docs && npm run build-docs",
    "prepublish": "npm run compile",
    "start": "webpack && node ./lib/bundle.js",
    "test-watch": "./node_modules/mocha/bin/_mocha --compilers js:babel-core/register ./test/unit-tests --watch",
    "test": "./node_modules/mocha/bin/_mocha --compilers js:babel-core/register ./test/unit-tests"
  },
  "dependencies": {
    "ramda": "^0.21.0"
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-core": "^6.7.6",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "eslint": "^2.6.0",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-friendly-formatter": "^1.2.2",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-promise": "^1.0.8",
    "eslint-plugin-react": "^4.2.3",
    "how-the-test-was-won": "^1.0.26",
    "isparta": "^4.0.0",
    "istanbul": "^0.4.3",
    "jsdoc": "^3.4.0",
    "jsdoc-babel": "^0.1.0",
    "json-loader": "^0.5.4",
    "minami": "^1.1.1",
    "mocha": "^2.4.5",
    "node-inspector": "^0.12.8",
    "webpack": "^1.13.0"
  }
}
